{
	"name": "iced-coffee-script",
	"description": "Syntax highlighting and \"go to symbol in file\" support for IcedCoffeeScript.",
	"author": "Micha≈Ç Zochniak",
	"license": "MIT",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/zapu/vscode-iced-coffee-script"
	},
	"publisher": "zapu",
	"categories": [],
	"keywords": [
		"coffeescript",
		"iced coffee script",
		"multi-root ready"
	],
	"engines": {
		"vscode": "^1.23.0"
	},
	"activationEvents": [
		"onLanguage:iced"
	],
	"main": "./client/out/extension",
	"contributes": {
		"languages": [
			{
				"id": "iced",
				"aliases": [
					"ICS",
					"icedcoffeescript",
					"icedcoffee"
				],
				"extensions": [
					".iced",
					".coffee",
					".Cakefile",
					".coffee.erb",
					".cson",
					".cjsx"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "iced",
				"scopeName": "source.coffee",
				"path": "./syntaxes/CoffeeScript.tmLanguage"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
		"compile:client": "tsc -p ./client/tsconfig.json",
		"compile:server": "make -C ./server build",
		"watch:client": "tsc -w -p ./client/tsconfig.json",
		"compile": "npm run compile:client && npm run compile:server",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"test": "sh ./scripts/e2e.sh"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.0",
		"@types/node": "^8.0.0",
		"typescript": "2.8.3"
	},
	"dependencies": {
		"vsce": "^1.48.0"
	}
}
